#neko
-neko bin/test_core.n
-main TestCore
-cp test/core
-cp src/core

-lib nanotest
-lib unifill

--next
-cmd neko "bin/test_core.n" 1>bin/core_neko_report.txt

--next
-lib nanotest
-D result_exit_code
--macro nanotest.NanoTestRunner.readResult('bin/core_neko_report.txt', ['test/core'], 'Neko')

# C Sharp
--next
-cs bin/cs
-main TestCore
-cp test/core
-cp src/core

-lib nanotest
-lib unifill

--next
-cmd ""./bin/cs/bin/TestCore.exe"" 1>bin/core_cs_report.txt

--next
-lib nanotest
-D result_exit_code
--macro nanotest.NanoTestRunner.readResult('bin/core_cs_report.txt', ['test/core'], 'C#')
