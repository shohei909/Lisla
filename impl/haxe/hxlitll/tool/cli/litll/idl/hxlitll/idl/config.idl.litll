[package hxlitll.idl]

[import litll.idl.library]
[import litll.idl.group]

[struct InputFile
    [target< TargetTuple]
    [filter..< FilterTuple]
]
[struct OutputFile
    [target< TargetTuple]
    [filter..< FilterTuple]
]

[tuple TargetTuple
    target [library< LibraryReference]
]
[tuple FilterTuple
    filter [filter< TypeGroupFilter]
]
