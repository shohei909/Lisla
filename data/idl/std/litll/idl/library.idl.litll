[package litll.idl]

[import litll.util.file]
[import litll.util.version]
[import litll.idl]

[struct LibraryConfig
    [version? Version]
    [extension..< FileExtensionTuple]
    [lib..< LibraryDependenceTuple]
]

[tuple FileExtensionTuple
    extension [target FileExtension] [type TypeReference]
]

[tuple LibraryDependenceTuple
    lib [library LibraryPath] [version Version]
]
