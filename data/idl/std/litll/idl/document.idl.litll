[package litll.idl]

[import litll.idl]
[import litll.core]
[import litll.util.highlight]

[struct TypeDefinitionDocument
    [document_tag.. TypeReference]

    [completion?]
    [completion_const.. Any]
    [completion_type.. TypeReference]
    [highlight.. HighlightScope]
]

[struct TypeReferenceDocument
    [completion?]
    [completion_const.. Any]
    [completion_type.. TypeReference]
]
