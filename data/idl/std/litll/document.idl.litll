[package litll]
[import litll.idl]

[struct Document
    [comment..< String]
]

[struct HeaderDocument
    [encoding DocumentEncoding]
    [license.. String]
    [author.. String]
    [schema TypeReference]
]

[enum DocumentEncoding
    utf8
]
