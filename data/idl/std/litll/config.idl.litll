/// 'IDL for Litll IDL configure file(.litllconfig.litll)'
/// [schema litll.idl.Idl]

[package litll]
[import litll.file]

[struct LitllConfig
    [idl.. LitllIdlSourceDirectory]
    [extention.. LitllSubextention]
]

[newtype LitllIdlSourceDirectory FilePath]
[newtype LitllSubextention String]
