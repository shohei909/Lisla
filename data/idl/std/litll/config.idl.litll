/// 'IDL for Litll IDL configure file(.litllconfig.litll)'
/// [schema litll.idl.Idl]
[package litll]

[import litll.file]
[import litll.idl]

[struct LitllConfig
    [idl.. LitllIdlSourceDirectory]
    [extension..< LitllFileExtensionConfig]
]

[newtype LitllIdlSourceDirectory FilePath]
[tuple LitllFileExtensionConfig 
    extension [target FileExtension] [type TypeReference]
]
