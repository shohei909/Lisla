[package '']
[import lisla.core]

[enum BracketExpr
    [value< [value Float64]]
    [expr< [expr SumExpr]]
]

[tuple ProductExpr
    [expr BracketExpr]
    [op..< ProductOp]
]

[enum ProductOp
    [mutiply: * [expr BracketExpr]]
    [divide: / [expr BracketExpr]]
]

[tuple SumExpr
    [expr< ProductExpr]
    [op..< SumOp]
]

[enum SumOp
    [plus: + [expr< ProductExpr]]
    [minus: - [expr< ProductExpr]]
]
