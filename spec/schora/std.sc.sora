[type [bool] 
    [or
        [const true]
        [const false]
    ]
]

[type [option [type t]]
    [or
        [tuple [t]]
        [unit]
    ]
]

[type [unit]
    [collection]
]

[type [tuple [types ts]] 
    [collection
        [required [ts]]
    ]
]

[type [array [type t]]
    [collection
        [rest [t]]
    ]
]

[type [variant [[string unquoted] label] [rest ts]]
    [tuple
        [label]
        [ts]
    ]
]

[type [map [type k] [type v]]
    [array
        [tuple [k] [v]]
    ]
]
